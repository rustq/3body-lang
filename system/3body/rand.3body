let rand = {
    "default": fn () {
        return {
            "getSuns": fn () {
                return random(4)
            }
        }
    },
    "custom": fn (count) {
        return {
            "getSuns": fn () {
                if (count < 3) {
                    return count;
                }
                return random(count)
            }
        }
    }
};

rand